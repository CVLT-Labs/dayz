<!-- Below are most (may have missed one or two) of the coordinates for Grocery Stores on Deer Isle. -->
<!-- I've included a custom event that should spawn some food items at most locations. -->
<!-- With 🤍 from rabbl. -->

<!-- Paste in events.xml -->
<event name="ItemGroceryStore">
    <nominal>151</nominal> <!-- Max Items + 25 Item Buffer -->
    <min>72</min> <!-- lootMin x Number of Children (ex: 4 x 18 = 72) -->
    <max>126</max> <!-- lootMax x Number of Children (ex: 7 x 18 = 126) -->
    <lifetime>7200</lifetime> <!-- Time (in seconds) for items to remain before respawn -->
    <restock>0</restock> <!-- Respawn items on server restart -->
    <saferadius>2</saferadius> <!-- Ensures some space between items, avoid stacking -->
    <distanceradius>6</distanceradius> <!-- Spawn items within a 5-meter radius of the position -->
    <cleanupradius>30</cleanupradius> <!-- Radius for cleaning up excess items -->
    <flags deletable="0" init_random="1" remove_damaged="1"/> <!-- Randomize item spawns, prevent damaged items -->
    <position>fixed</position>
    <limit>mixed</limit>
    <active>1</active> <!-- Event is active -->
    <children>
        <child lootmax="7" lootmin="4" max="150" min="75" type="BakedBeansCan"/> 
        <child lootmax="7" lootmin="4" max="150" min="75" type="CatFoodCan"/>
        <child lootmax="7" lootmin="4" max="150" min="75" type="PeachesCan"/>
        <child lootmax="7" lootmin="4" max="150" min="75" type="SardinesCan"/>
        <child lootmax="7" lootmin="4" max="150" min="75" type="TunaCan"/>
        <child lootmax="7" lootmin="4" max="150" min="75" type="PastaCan"/>
        <child lootmax="7" lootmin="4" max="150" min="75" type="SpaghettiCan"/>
        <child lootmax="7" lootmin="4" max="150" min="75" type="TacticalBaconCan"/>
        <child lootmax="7" lootmin="4" max="150" min="75" type="DogFoodCan"/>
        <child lootmax="7" lootmin="4" max="150" min="75" type="PowderedMilk"/>
        <child lootmax="7" lootmin="4" max="150" min="75" type="Rice"/>
        <child lootmax="7" lootmin="4" max="150" min="75" type="Canteen"/>
        <child lootmax="7" lootmin="4" max="150" min="75" type="WaterBottle"/>
        <child lootmax="7" lootmin="4" max="150" min="75" type="Pipsi"/>
        <child lootmax="7" lootmin="4" max="150" min="75" type="Kvass"/>
        <child lootmax="7" lootmin="4" max="150" min="75" type="SodaCan_Cola"/>
        <child lootmax="7" lootmin="4" max="150" min="75" type="SodaCan_Spite"/>
        <child lootmax="7" lootmin="4" max="150" min="75" type="SodaCan_Fronta"/>
    </children>
</event>

